<Window x:Class="Lecture6ListBox.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lecture6ListBox"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Label x:Name="NumberLabel" Content="Number" HorizontalAlignment="Left" Margin="62,62,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="numberTextBox" HorizontalAlignment="Left" Height="23" Margin="121,65,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="checkButton" Content="Check" HorizontalAlignment="Left" Margin="62,126,0,0" VerticalAlignment="Top" Width="75" Click="checkButton_Click"/>
        <TextBox x:Name="outputTextBox" HorizontalAlignment="Left" Height="26" Margin="62,200,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="101" IsEnabled="False"/>
        <ListBox x:Name="operatorListBox" HorizontalAlignment="Left" Height="100" Margin="241,126,0,0" VerticalAlignment="Top" Width="100"/>
    </Grid>
</Window>
